
<%# render :partial => 'atrium_showcases/navigation_browse_levels', :locals=>{:exhibit_number=>@exhibit.set_number, :browse_levels=>@exhibit.browse_levels, :browse_facets=>@exhibit.browse_facet_names}%>
<%# debug "Browse PAGE:#{@atrium_showcase.inspect}" %>
<li>
<%- if current_user && params[:edit_showcase] && params[:controller] == "atrium_collections"-%>
  <%- if !@atrium_showcase -%>
    <%= link_to 'Configure Collection', new_atrium_collection_atrium_showcases_path(@atrium_collection, :facet_selection=>params[:f]), :class=>"customize"%>
  <%- else %>
    <%= link_to 'View this collection', atrium_collection_path(:id => params[:id], :f=>params[:f])%>
  <%- end %>
<%- end %>
</li>
<%- unless @exhibit_navigation_data.nil? || @exhibit_navigation_data.empty? -%>
    <!-- recursively render the browse sets-->
  <%- @exhibit_navigation_data.each do |exhibit| -%>
    <%- if @exhibit && exhibit.set_number == @exhibit.set_number  %>
    <%# debug "#{exhibit.browse_levels}, @exhibit: #{@exhibit.browse_levels.inspect}" %>
      <%- unless exhibit.browse_levels.nil? || exhibit.browse_levels.empty? -%>
        <%# exhibit.set_number%>
        <%= render :partial => 'atrium_showcases/navigation_browse_levels', :locals=>{:exhibit_number=>exhibit.set_number, :browse_levels=>exhibit.browse_levels, :browse_facets=>exhibit.browse_facet_names}%>
      <%- end -%>
    <%- end -%>
  <%- end -%>
<%- end -%>



